          coursename2 = "도술학개론",
          pname2 = "홍길동 교수님",
          rnum2 = 13,

          coursename3 = "도술학개론",
          pname3 = "홍길동 교수님",
          rnum3 = 13;

      var ecoursename1 = document.getElementById('coursename1'),
          epname1 = document.getElementById('pname1'),
          ernum1 = document.getElementById('rnum1'),

          ecoursename2 = document.getElementById('coursename2'),
          epname2 = document.getElementById('pname2'),
          ernum2 = document.getElementById('rnum2'),

          ecoursename3 = document.getElementById('coursename3'),
          epname3 = document.getElementById('pname3'),
          ernum3 = document.getElementById('rnum3');

      ecoursename1.textContent = coursename1;
      epname1.textContent = pname1;
      ernum1.textContent = rnum1;

      ecoursename2.textContent = coursename2;
      epname2.textContent = pname2;
      ernum2.textContent = rnum2;

      ecoursename3.textContent = coursename3;
      epname3.textContent = pname3;
      ernum3.textContent = rnum3;

    </script>

